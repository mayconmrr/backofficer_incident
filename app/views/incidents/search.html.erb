<div class="panel panel-primary">
  <div class="panel-heading" ><h3>Listagem de Requisições</h3></div>
</div>

<%= render 'search' %>

<div class='basic-grey'>
  <div class='centered' id='list_incidents'>
    <table>
      <thead>
        <tr>
          <th>ID</th>
          <th>Resumo do Problema</th>
          <th>Nível de Prioridade</th>
          <th>Reportador por</th>
          <th>Aberto Em</th>
          <th>Última Atualização</th>
          <th>Status</th>
          <th>Analista</th>
          <th colspan="3"></th>
        </tr>
      </thead>
      <tbody>
        <%= render partial: 'incidents/partials/list_incidents', locals: { incidents: @incidents } %>
      </tbody>
    </table>
  </div>
</div>

<div class="panel-heading" align="center"><%= will_paginate @incidents %></div>
